{
    "manifest_version": 2,
    "name": "Injector",
    "short_name":"*_TI_*",
    "description": "Sources files and injects them authomatically.",
    "version": "0.0.01",
    "icons": {
    
    },
    "browser_action": {
        "default_title": "Injector",
        "default_popup": "html/popup.html",
        "default_action": "modules/background.js"
    },
    "content_scripts": [
        {
            "js": [
                "chrome_extension.js",
                "modules/index.js"
            ],
            "matches": [
                "https://*/*",
                "http://*/*"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [

    ],
    "permissions": [
        "<all_urls>",
        "https://*/*",
        "http://*/*",
        "tabs",
        "background"
    ]
}